<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://msi.umn.edu/tropix/proteomics/itraqquantitation/options"
            xmlns="http://msi.umn.edu/tropix/proteomics/itraqquantitation/options"
            elementFormDefault="qualified">

  <xsd:element name="quantificationType" type="QuantificationType" />
  
  <xsd:simpleType name="QuantificationType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="FOUR_PLEX"/>
      <xsd:enumeration value="EIGHT_PLEX"/>
      <xsd:enumeration value="TMT_TWO_PLEX"/>
      <xsd:enumeration value="TMT_SIX_PLEX"/>
    </xsd:restriction>
  </xsd:simpleType> 

</xsd:schema>
