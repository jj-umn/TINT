<!--

    // Information from table on page:
    // http://www.matrixscience.com/help/search_field_help.html

    Trypsin  KR  P  CTERM
    Arg-C R P CTERM
    Asp-N BD   NTERM
    Asp-N_ambic DE   NTERM
    Chymotrypsin FYWL P CTERM
    CNBr M   CTERM
    Formic_acid D   CTERM
    Lys-C K P CTERM
    Lys-C/P K   CTERM
    PepsinA FL   CTERM
    Tryp-CNBr KRM P CTERM
    TrypChymo FYWLKR P CTERM
    Trypsin/P KR   CTERM
    V8-DE BDEZ P CTERM
    V8-E EZ P CTERM

-->

<!--
    https://proteomics.fhcrc.org/CPAS/Wiki/home/FHCRC%20Help/page.view?name=alternate%20enzyme%20parameters
    * nonspecific - [X]|[X]
    * trypsin - [KR]|{P}
    * trypsin_k - [K]|{P}
    * trypsin_r - [R]|{P}
    * trypsin/cnbr - [KMR]|{P}
    * chymotrypsin - [FMWY]|{P}
    * clostripain - [R]|[X]
    * iodosobenzoate - [W]|[X]
    * protein_endopeptidase - [P]|[X]
    * staph_protease - [E]|[X]
    * aspn - [X]|[D]
    * elastase - [AGILV]|{P}
    * gluc - [DE]|{P}
    * gluc_bicarb - [E]|{P}
    * cnbr - [M]|{P}
    * elastase-tryp-chymo - [AFIKLRVWY]|{P}
    * chymotryp-modified - [FLWY]|{P}
    * cyanogen_bromide - [M]|[X]
-->

<enzymeSet xmlns="http://msi.umn.edu/proteomics">
  <!-- http://en.wikipedia.org/wiki/Amino_acid B is either D or N and Z is  E or Q -->
  <enzyme name="Trypsin"      doCleave="KR"     doNotCleave="P" term="CTERM" /> <!-- [RK]|{P} -->
  <enzyme name="Arg-C"        doCleave="R"      doNotCleave="P" term="CTERM" /> <!-- [R]|{P} -->
  <!-- <enzyme name="Asp-N"        doCleave="BD"     doNotCleave=""  term="NTERM" />  --> <!-- [X]|[D] -->
  <enzyme name="Asp-N" doCleave="D" doNotCleave="" term="NTERM" />
  <enzyme name="Asp-N_ambic"  doCleave="DE"     doNotCleave=""  term="NTERM" />
  <enzyme name="Chymotrypsin" doCleave="FYWL"   doNotCleave="P" term="CTERM" />
  <enzyme name="CNBr"         doCleave="M"      doNotCleave=""  term="CTERM" /> <!-- cyanogen bromide [M]|[X] -->
  <enzyme name="Formic_acid"  doCleave="D"      doNotCleave=""  term="CTERM" />
  <enzyme name="Lys-C"        doCleave="K"      doNotCleave="P" term="CTERM" /> 
  <!-- endo-lys-c was [K]|{X} -->
  <enzyme name="Lys-C/P"      doCleave="K"      doNotCleave=""  term="CTERM" />
  <enzyme name="PepsinA"      doCleave="FL"     doNotCleave=""  term="CTERM" />
  <enzyme name="Tryp-CNBr"    doCleave="KRM"    doNotCleave="P" term="CTERM" />
  <enzyme name="TrypChymo"    doCleave="FYWLKR" doNotCleave="P" term="CTERM" />
  <enzyme name="Trypsin/P"    doCleave="KR"     doNotCleave=""  term="CTERM" />
  <enzyme name="V8-DE"        doCleave="BDEZ"   doNotCleave="P" term="CTERM" />
  <enzyme name="V8-E"         doCleave="EZ"     doNotCleave="P" term="CTERM" /> <!-- I think this is endoproteinase (glu-c), [E]|{X} -->
  <enzyme name="No_Enzyme"    doCleave=""       doNotCleave=""  term="NTERM" /> 
</enzymeSet>
