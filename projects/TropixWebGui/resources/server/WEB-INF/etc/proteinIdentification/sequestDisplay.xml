<?xml version="1.0"  encoding="UTF-8" standalone="yes"?>
<formConfiguration>
  <tabs>
  <tab name="I">
    <tooltip>General parameters (1 / 2)</tooltip>
    <verticalPanel>
      <entry>
    <table>
    <row>
      <cell ><label  text="Peptide Tolerance" /></cell>
      <cell><input widget="textbox" parameter="peptideMassTolerance" width="60" /></cell>
      <cell><info  parameter="peptideMassTolerance" /></cell>
    </row>
    <row>  
      <cell ><label  text="Peptide Units" /></cell>
      <cell><input widget="selection" parameter="peptideMassUnits" /></cell>
      <cell><info  parameter="peptideMassUnits" /></cell>
    </row>
    <row>
      <cell ><label  text="Fragment Tolerance" /></cell>
      <cell><input widget="textbox" parameter="fragmentMassTolerance" width="60" /></cell>
      <cell><info  parameter="fragmentMassTolerance" /></cell>
    </row>
    <row>
      <cell ><label  text="Fragment Type" /></cell>
      <cell><input widget="selection" parameter="massTypeFragment" /></cell>
      <cell><info  parameter="massTypeFragment" /></cell>
    </row>
    <row>
      <cell ><label  text="Parent Type" /></cell>
      <cell><input widget="selection" parameter="massTypeParent" /></cell>
      <cell><info  parameter="massTypeParent" /></cell>
    </row>
    <row>
      <cell ><label  text="Max Cleavage Sites" /></cell>
       <cell><input widget="textbox" parameter="internalCleavageSites" width="15" /></cell>
       <cell><info  parameter="internalCleavageSites" /></cell> 
    </row>
    </table>
    </entry>
      <entry>
    <table>
    <row>
    <cell ><label  text="Enzyme Name" /></cell>
    <cell><input widget="selection" parameter="enzymeName" /></cell>
    <cell ><label  text="Enzyme Limit" /></cell>
    <cell><input widget="selection" parameter="enzymeLimit"  /></cell>
    </row>
    </table>
      </entry>
    </verticalPanel>
  </tab>

  <tab name="II">
    <tooltip>General parameters (2 / 2)</tooltip>
    <table>
    <row>
      <cell ><label  text="Scored Results" /></cell>
      <cell><input widget="textbox" parameter="numResults" width="50" /></cell>
      <cell><info  parameter="numResults" /></cell>
      
      <cell ><label  text="Remove Precursor" /></cell>
      <cell><input widget="checkbox" parameter="removePrecursorPeak" /></cell>
      <cell><info  parameter="removePrecursorPeak" /></cell>
         
    </row>
    <row>
      <cell ><label  text="Output Lines" /></cell>
      <cell><input widget="textbox" parameter="numOutputLines" width="50"/></cell>
      <cell><info  parameter="numOutputLines" /></cell>
      
      <cell ><label  text="Ion Cutoff %" /></cell>
      <cell><input widget="textbox" parameter="ionCutoffPercentage" width="50" /></cell>
      <cell><info  parameter="ionCutoffPercentage" /></cell>
            
    </row>
    <row>
      <cell ><label  text="Description Lines" /></cell>
      <cell><input widget="textbox" parameter="numDescriptionLines" width="50"/></cell>
      <cell><info  parameter="numDescriptionLines" /></cell>
      
      <cell ><label  text="Peak Count" /></cell>
      <cell><input widget="textbox" parameter="matchPeakCount" width="50" /></cell>
      <cell><info  parameter="matchPeakCount" /></cell>      
    </row>
    <row>
      <cell ><label  text="Show Fragment Ions" /></cell>
      <cell><input widget="checkbox" parameter="showFragmentIons" /></cell>
      <cell><info  parameter="showFragmentIons" /></cell>

      <cell ><label  text="Peak Allowed Error" /></cell>
      <cell><input widget="textbox" parameter="matchPeakAllowedError" width="50" /></cell>
      <cell><info   parameter="matchPeakAllowedError" /></cell>      
    </row>
    <row>
      <cell ><label  text="Output Duplicates" /></cell>
      <cell><input widget="textbox" parameter="printDuplicateReferences" width="30" /></cell>
      <cell><info  parameter="printDuplicateReferences" /></cell>
      
      <cell ><label  text="Peak Tolerance" /></cell>
      <cell><input widget="textbox" parameter="matchPeakTolerance" width="50" /></cell>
      <cell><info  parameter="matchPeakTolerance" /></cell>      
    </row>
    <row>
      <cell ><label  text="Differentials Per Peptide" /></cell>
      <cell><input widget="textbox" parameter="maxNumDifferentialsPerPeptide" width="15" /></cell>
      <cell><info  parameter="maxNumDifferentialsPerPeptide" /></cell>
      
      <cell ><label  text="Digest Mass Range" /></cell>
      <cell><input widget="textbox" parameter="digestMassRangeLower" width="50" /></cell>
      <cell><input widget="textbox" parameter="digestMassRangeUpper" width="50" /></cell>      
    </row>
    <row>
      <cell ><label  text="Normalize xcorr" /></cell>
      <cell><input widget="checkbox" parameter="normalizeXCorr" /></cell>
      <cell><info parameter="normalizeXCorr" /></cell>
      
      <cell ><label  text="Protein Mass Filter" /></cell>
      <cell><input widget="textbox" parameter="proteinMassFilterLower" width="50" /></cell>
      <cell><input widget="textbox" parameter="proteinMassFilterUpper" width="50" /></cell>       
    </row>
    </table>
  </tab>
  
