<?xml version="1.0"  encoding="UTF-8" standalone="yes"?>
<formConfiguration>
  <tabs>
  <tab name="Protein">
    <table>
      <row>
        <cell><label text="Cleavage Agent" /></cell>
        <cell><input widget="selection" parameter="enzyme" /></cell>
        <cell><label text="Cleavage Semi" /></cell>
        <cell><input widget="checkbox" parameter="proteinCleavageSemi" /></cell>
      </row>
      <row>
        <cell><label text="Cleavage C-Terminal Mass Change" /></cell>
        <cell><input widget="textbox" parameter="proteinCleavageCTerminalMassChange" width="60"  /></cell>
        <cell><label text="Cleavage N-Terminal Mass Change" /></cell>
        <cell><input widget="textbox" parameter="proteinCleavageNTerminalMassChange"  width="60"  /></cell>
      </row>
      <row>
        <cell><label text="C-Terminal Residue Modification Mass" /></cell>
        <cell><input widget="textbox" parameter="proteinCTerminalResidueModificationMass"  width="60"  /></cell>
        <cell><label text="N-Terminal Residue Modification Mass" width="60"  /></cell>
        <cell><input widget="textbox" parameter="proteinNTerminalResidueModificationMass" width="60" /></cell>
      </row>
      <row>
        <cell><label text="Cyclic Permutations" /></cell>
        <cell><input widget="checkbox" parameter="scoringCyclicPermutation" /></cell>
        <cell><label text="Include Reverse" /></cell>
        <cell><input widget="checkbox" parameter="scoringIncludeReverse" /></cell>
      </row>
      <row>
        <cell><label text="Max Missed Cleavage Sites" /></cell>
        <cell><input widget="textbox" parameter="scoringMaximumMissedCleavageSites" width="60"  /></cell>
      </row>
      </table>
  </tab>
  <tab name="Spectrum">
    <table>
      <row>
        <cell><label text="Fragment Mass Error" /></cell>
        <cell><input widget="textbox" parameter="spectrumFragmentMassError" width="60" /></cell>
        <cell><label text="Framment Mass Error Units" /></cell>
        <cell><input widget="selection" parameter="spectrumFragmentMassErrorUnits" /></cell>
      </row>
      <row>
        <cell><label text="Fragment Mass Error Type" /></cell>
        <cell><input widget="selection" parameter="spectrumFragmentMassType" /></cell>
        <cell><label text="Parent Mass Isotope Error " /></cell>
        <cell><input widget="checkbox" parameter="spectrumParentMonoisotopicMassIsotopeError" /></cell>
      </row>
      <row>
        <cell><label text="Parent Mass Error Plus" /></cell>
        <cell><input widget="textbox" parameter="spectrumParentMonoisotopicMassErrorPlus" width="60"  /></cell>
        <cell><label text="Parent Mass Error Minus" /></cell>
        <cell><input widget="textbox" parameter="spectrumParentMonoisotopicMassErrorMinus" width="60" /></cell>
      </row>      
      <row>
        <cell><label text="Parent Mass Error Units" /></cell>
        <cell><input widget="selection" parameter="spectrumParentMonoisotopicMassErrorUnits" width="60"  /></cell>
        <cell><label text="Use Neutral Loss Window" /></cell>
        <cell><input widget="checkbox" parameter="spectrumUseNeutralLossWindow"  /></cell>
      </row>      
      <row>
        <cell><label text="Neutral Loss Window (if used)" /></cell>
        <cell><input widget="textbox" parameter="spectrumNeutralLossWindow" width="60" /></cell>
        <cell><label text="Neutral Loss Mass (if used)" /></cell>
        <cell><input widget="textbox" parameter="spectrumNeutralLossMass" width="60" /></cell>
      </row>
      <row>
        <cell><label text="Use Contrast Angle" /></cell>
        <cell><input widget="checkbox" parameter="spectrumUseContrastAngle" /></cell>
        <cell><label text="Contrast Angle (if used)" /></cell>
        <cell><input widget="textbox" parameter="spectrumContrastAngle" width="60" /></cell>
      </row>
      <row>
        <cell><label text="Maximum Parent Charge" /></cell>
        <cell><input widget="textbox" parameter="spectrumMaximumParentCharge" width="60" /></cell>
      </row>      
      </table>
  </tab>
  <tab name="Ions">
    <table>
      <row>
        <cell><label text="Score A Ions" /></cell>
        <cell><input widget="checkbox" parameter="scoringAIons" width="60" /></cell>
        <cell><label text="Score B Ions" /></cell>
        <cell><input widget="checkbox" parameter="scoringBIons" width="60"  /></cell>
     </row>
     <row>
        <cell><label text="Score C Ions" /></cell>
        <cell><input widget="checkbox" parameter="scoringCIons" width="60" /></cell>
        <cell><label text="Score X Ions" /></cell>
        <cell><input widget="checkbox" parameter="scoringXIons" width="60" /></cell>
     </row>
     <row>
        <cell><label text="Score Y Ions" /></cell>
        <cell><input widget="checkbox" parameter="scoringYIons" width="60" /></cell>
        <cell><label text="Score Z Ions" /></cell>
        <cell><input widget="checkbox" parameter="scoringZIons" width="60" /></cell>
      </row>
     </table>
  </tab>

