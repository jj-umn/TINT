  <tab name="Refinement">
    <tooltip>Refinement</tooltip>     
    <table>
    <row>
      <cell><label text="Use Refinement" /></cell>
      <cell><input widget="checkbox" parameter="refine" /></cell>
      <cell><label text="Point Mutations" /></cell>
      <cell><input widget="checkbox" parameter="refinePointMutations" /></cell>
    </row>
    <row>
      <cell><label text="Cleavage Semi" /></cell>
      <cell><input widget="checkbox" parameter="refineCleavageSemi" /></cell>
      <cell><label text="Test for SAPS" /></cell>
      <cell><input widget="checkbox" parameter="refineSingleAminoAcidPolymorphisms" /></cell>
    </row>
    <row>
      <cell><label text="Spectrum Synthesis" /></cell>
      <cell><input widget="checkbox" parameter="refineSpectrumSynthesis" /></cell>
      <cell><label text="Unanticipated Cleavage" /></cell>
      <cell><input widget="checkbox" parameter="refineUnanticipatedCleavage" /></cell>
    </row>
    <row>
      <cell><label text="Maximum Expectation Value" /></cell>
      <cell><input widget="textbox" parameter="refineMaximumValidExpectationValue" width="50" /></cell>
    </row>
    </table>
  </tab>

